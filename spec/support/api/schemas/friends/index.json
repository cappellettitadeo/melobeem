{
  "type": "object",
  "required": ["users", "meta"],
  "properties": {
    "users": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "name", "avatar_url", "source_type", "invitation_prize"],
        "properties": {
          "id": {"type": "integer"},
          "name": {"type": "string"},
          "avatar_url": {"type": "string"},
          "source_type": {"type": "string"},
          "invitation_prize": {"type":  "nullable"}
        }
      }
    },
    "meta": {
      "type": "object",
      "required": ["total_count", "per", "page"],
      "properties": {
        "total_count": {"type": "integer"},
        "per": {"type": "integer"},
        "page": {"type": "integer"}
      }
    }
  }
}
