{
  "type": "object",
  "required": ["entries", "meta"],
  "properties": {
    "entries": {
      "type": "array",
      "items": {
        "type": "object",
        "required": ["id", "gender", "name", "image_url"],
        "properties": {
          "id": {"type": "integer"},
          "gender": {"type": "string"},
          "name": {"type": "string"},
          "image_url": {"type": "string"}
        }
      }
    },
    "meta": {
      "type": "object",
      "required": ["total_count", "per", "page"],
      "properties": {
        "total_count": {"type": "integer"},
        "per": {"type": "integer"},
        "page": {"type": "integer"}
      }
    }
  }
}
